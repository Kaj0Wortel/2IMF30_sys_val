nu X(UnloadingCarts: List(Nat)=[]) . 
	forall c1: Nat . val(c1<4) => [unloadCart(c1, true)]X(add(UnloadingCarts, c1)) &&
	forall c2: Nat . val(c2<4) => [unloadCart(c2, false)]X(remove(UnloadingCarts, c2)) &&
	forall c3: Nat . val(c3<4) => [moveCartForward(c3) || moveCartBackward(c3)] val(!inList(UnloadingCarts, c3)) && X(UnloadingCarts) &&
	forall n: Nat . val(n<4) => [!(unloadCart(n,true)||unloadCart(n,false)||moveCartForward(n)||moveCartBackward(n))]X(UnloadingCarts)